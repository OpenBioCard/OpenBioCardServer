{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "Type": "SQLite",
    "ConnectionString": "Data Source=development.db"
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:8787"
    ]
  },
  "CacheSettings": {
    "UseRedis": false,
    "RedisConnectionString": "localhost:6379",
    "InstanceName": "OpenBioCard:",
    "ProfileCacheSizeLimit": 200,
    "ExpirationMinutes": 5
  },
  "CompressionSettings": {
    "Enabled": true,
    "EnableForHttps": true,
    "Level": "Fastest"
  },
  "AuthSettings": {
    "RootUsername": "root",
    "RootPassword": "password"
  },
  "AssetSettings": {
    "MaxFileSizeMB": 5,
    "AllowedImageTypes": [
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/webp"
    ]
  }
}
